<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="540.0" prefWidth="960.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="gui.addRecord.AddRecordScreenController">
    <top>
        <HBox alignment="CENTER" spacing="250.0">
            <Button fx:id="backBtn" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#backBtnClicked"
                    prefHeight="26.0"
                    prefWidth="114.0" text="Back" textAlignment="CENTER">
                <HBox.margin>
                    <Insets bottom="50.0" top="50"/>
                </HBox.margin>
            </Button>
            <Button alignment="TOP_CENTER" mnemonicParsing="false" prefHeight="26.0" prefWidth="114.0" text="Save"
                    onAction="#saveBtnClicked"
                    textAlignment="CENTER">
                <HBox.margin>
                    <Insets bottom="50.0" top="50"/>
                </HBox.margin>
            </Button>
        </HBox>
    </top>
    <center>
        <TableView fx:id="tableView" prefHeight="381.0" prefWidth="960.0" BorderPane.alignment="CENTER"/>
    </center>
</BorderPane>
